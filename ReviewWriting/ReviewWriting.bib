	keywords = {alpha cell marker, alpha-cells, beta-cells, differentiation, gene-expression, insulin-secretion, islets, organogenesis, pancreas development, protein, rna, single cell RNA-Seq, transthyretin},
	pages = {105--114},
	file = {Full Text PDF:D\:\\OneDrive - mails.ucas.edu.cn\\Papers\\storage\\ERNY5KRZ\\Single cell transcriptomic profiling of mouse panc.pdf:application/pdf}
}

@article{gao_tracing_2018,
