	month = oct,
	year = {2016},
	keywords = {islets of Langerhans, pancreas, sequencing, single-cell transcriptomics},
	pages = {385--394.e3},
	file = {ScienceDirect Full Text PDF:D\:\\OneDrive - mails.ucas.edu.cn\\Papers\\storage\\VSTKH5ER\\A Single-Cell Transcriptome Atlas of the Human Pan.pdf:application/pdf;ScienceDirect Full Text PDF:D\:\\OneDrive - mails.ucas.edu.cn\\Papers\\storage\\ZEN46W75\\A Single-Cell Transcriptome Atlas of the Human Pan.pdf:application/pdf}
