	pmcid = {PMC2673761},
	keywords = {Animals, Animals, Newborn, Antigens, Differentiation, Bile Ducts, Cell Differentiation, Hepatocytes, Liver, Mice, Mice, Mutant Strains, Morphogenesis, Receptors, Notch, Signal Transduction},
	pages = {1727--1739}
}

