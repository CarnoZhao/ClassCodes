	pages = {776--779},
	file = {Full Text PDF:D\:\\OneDrive - mails.ucas.edu.cn\\Papers\\storage\\XPDS2DNB\\Massively Parallel Single-Cell RNA-Seq for Marker-.pdf:application/pdf}
