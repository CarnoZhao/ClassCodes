	month = mar,
	year = {2015},
	pmid = {25700174},
	pages = {1138--1142},
	file = {Cell types in the mouse cortex and hippocampus rev.pdf:D\:\\OneDrive - mails.ucas.edu.cn\\Papers\\storage\\LIMD2YYI\\Cell types in the mouse cortex and hippocampus rev.pdf:application/pdf;Full Text PDF:D\:\\OneDrive - mails.ucas.edu.cn\\Papers\\storage\\364NERNH\\Cell types in the mouse cortex and hippocampus rev.pdf:application/pdf}
}

