	year = {2016},
	pages = {346--360.e4},
	file = {ScienceDirect Full Text PDF:D\:\\OneDrive - mails.ucas.edu.cn\\Papers\\storage\\P4S9DN4E\\A Single-Cell Transcriptomic Map of the Human and .pdf:application/pdf}
}

@article{muraro_single-cell_2016,
	title = {A {Single}-{Cell} {Transcriptome} {Atlas} of the {Human} {Pancreas}},
